<%for(const comment of comments){%>
<hr>
<span class="date pull-right"><%= comment.created_date %></span>
<input class="answered-rating-input" name="rating" type="text" title="" value="4" readonly />
<span class="by"><%= comment.fullname %></span>
<p><%- helpers.santitizeData(comment.description) %></p>
<%}%>